{
  "name": "vue-template",
  "version": "1.5.0",
  "description": "A customized Vue.js project template",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode production --env.message",
    "build:monitor": "npm run build -- --env.monitor",
    "build:profile": "webpack --mode production --profile --json > stats/profile.json",
    "build:styleguide": "npm run build -- --env.styleguide && vue-styleguidist build",
    "build:watch": "npm run build -- --env.watch",
    "eslint": "eslint --cache --ext .js,.vue app test/unit/specs; exit 0;",
    "eslint:fix": "eslint --ext .js,.vue app test/unit/specs --fix; exit 0;",
    "stylelint": "stylelint --cache 'app/**/*.?(vue|scss)'; exit 0",
    "stylelint:fix": "echo \"Using stylelint --fix is not a good idea when using *.vue files...\"",
    "lint": "npm run eslint && npm run stylelint",
    "license": "license-checker --summary",
    "license:stats": "license-checker --csv --out stats/license.csv",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --config package.json; exit 0;",
    "test:travis": "eslint --ext .js,.vue app test/unit/specs && stylelint 'app/**/*.?(vue|scss)' && jest --config package.json",
    "dev": "webpack-dev-server --mode development",
    "dev:s": "npm run dev:styleguide",
    "dev:styleguide": "vue-styleguidist server",
    "postinstall": "echo \"\\x1b[44m Please update package.md after installing new packages and check build output for malicious code! \\x1b[0m\"",
    "postuninstall": "echo \"\\x1b[44m Please update package.md after uninstalling packages! \\x1b[0m\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patric-eberle/vue-template.git"
  },
  "engines": {
    "node": ">=8.10.0 <9.0.0",
    "npm": ">=5.7.1"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "Patric Eberle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/$1"
    },
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "setupFiles": [
      "<rootDir>/tests/unit/setup"
    ],
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "collectCoverageFrom": [
      "app/components/**/*.{js,vue}",
      "app/plugins/**/*.{js,vue}",
      "!app/main.js",
      "!**/node_modules/**"
    ],
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@babel/polyfill": "~7.0.0",
    "axios": "~0.18.0",
    "css-vars-ponyfill": "~1.11.1",
    "lazysizes": "~4.0.2",
    "lodash": "~4.17.10",
    "material-design-icons-iconfont": "~3.0.3",
    "moment": "~2.22.2",
    "picturefill": "~3.0.2",
    "portal-vue": "~1.4.0",
    "simplebar": "~2.6.1",
    "smoothscroll-polyfill": "~0.4.3",
    "swiper": "~4.2.6",
    "vue": "~2.5.17",
    "vue-bem-cn": "~1.1.1",
    "vue-i18n": "~8.2.1",
    "vue-js-modal": "~1.3.26",
    "vue-router": "~3.0.1",
    "vue-tabs-component": "~1.4.0",
    "vuetify": "~1.1.7",
    "vuex": "~3.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.1.2",
    "@babel/plugin-syntax-dynamic-import": "~7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "~7.0.0",
    "@babel/preset-env": "~7.1.0",
    "axios-mock-adapter": "~1.15.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~8.2.3",
    "babel-jest": "~22.4.4",
    "babel-loader": "~8.0.4",
    "babel-plugin-dynamic-import-node": "~2.2.0",
    "babel-plugin-transform-imports": "~1.5.1",
    "copy-webpack-plugin": "~4.5.3",
    "css-loader": "~1.0.0",
    "eslint": "~4.18.2",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-config-valantic": "git+https://github.com/valantic/eslint-config-valantic.git#4.5.0",
    "eslint-import-resolver-webpack": "~0.10.1",
    "eslint-loader": "~2.1.1",
    "eslint-plugin-import": "~2.9.0",
    "eslint-plugin-vue": "~4.4.0",
    "file-loader": "~2.0.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "image-webpack-loader": "~4.4.0",
    "jest": "~22.4.2",
    "launch-editor-middleware": "~2.2.1",
    "license-checker": "~18.0.0",
    "mini-css-extract-plugin": "~0.4.4",
    "node-sass": "~4.9.4",
    "postcss-critical-split": "~2.5.1",
    "postcss-html": "~0.14.0",
    "postcss-pipeline-webpack-plugin": "~4.1.0",
    "regenerator-runtime": "~0.12.1",
    "rimraf": "~2.6.2",
    "sass-loader": "~7.1.0",
    "sass-resources-loader": "~1.3.4",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-config-valantic": "git+https://github.com/valantic/stylelint-config-valantic.git#3.2.0",
    "stylelint-webpack-plugin": "~0.10.5",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "typescript": "~2.7.2",
    "uglifyjs-webpack-plugin": "~2.0.1",
    "url-loader": "~1.1.2",
    "vue-jest": "~2.1.1",
    "vue-loader": "~15.4.2",
    "vue-markdown-loader": "~2.4.1",
    "vue-styleguidist": "~1.8.3",
    "webpack": "~4.20.2",
    "webpack-auto-inject-version": "~1.1.0",
    "webpack-clean": "~1.2.3",
    "webpack-cli": "~3.1.2",
    "webpack-dev-server": "~3.1.9",
    "webpack-monitor": "~1.0.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
