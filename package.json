    "eslint-import-resolver-webpack": "~0.13.2",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-vue": "~9.7.0",
    "fork-ts-checker-webpack-plugin": "~6.5.2",
    "html-webpack-plugin": "~5.5.0",
    "husky": "~8.0.1",
