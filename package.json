{
  "name": "vue-template",
  "version": "7.0.0",
  "description": "A customized Vue.js project template",
  "lastDependencyUpdate": "03.11.2022",
  "scripts": {
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "vite",
    "dev:w": "webpack-dev-server --mode development",
    "jest": "jest tests --config jest.config.js --silent",
    "jest:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand tests/unit/specs/<path-to-your-test-file>.ts",
    "fix:eslint": "npm run lint:eslint -- --config .eslintrc.fix.js --cache=false --fix",
    "fix:stylelint": "npm run lint:stylelint -- --cache=false --config .stylelintrc.fix.js --fix",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:stylelint": "stylelint 'src/**/*.?(vue|scss)'",
    "lint:eslint": "eslint --ext .ts,.js,.vue ./",
    "test": "npm run tsc && npm run lint && npm run test:unit -- --watch=false",
    "test:unit": "vitest --dir tests",
    "test:w": "npm run tsc && npm run lint && npm run jest; exit 0;",
    "build": "vite build --mode=app",
    "build:w": "NODE_ENV=production webpack --mode production",
    "build:profile": "npm run build -- --mode='profile'",
    "build:profile:w": "NODE_ENV=production webpack --mode production --profile",
    "build:watch": "npm run build -- --watch",
    "build:watch:W": "NODE_ENV=production webpack --mode production --watch",
    "build:icons": "svg-sprite --config .svg-sprite.json src/assets/icons/*.svg",
    "serve": "vite preview",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "profile": "rollup-plugin-visualizer",
    "tsc": "vue-tsc --noEmit"
  },
  "webpack": {
    "buildPath": "dist",
    "localDist": "webpack-dist",
    "productionPath": "/",
    "styleguideBuildPath": "./dist",
    "styleguidePath": "/",
    "developmentPath": "/",
    "outputAssetsFolder": "assets",
    "appName": "app",
    "filePrefix": "",
    "themeSource": "src/setup/scss/themes/",
    "themeFiles": [
      "theme-01",
      "theme-02"
    ],
    "devPort": 9090
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": ">=18 <19",
    "npm": ">=8.19"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "@vuelidate/core": "~2.0.0",
    "@vuelidate/validators": "~2.0.0",
    "axios": "~1.1.2",
    "core-js": "~3.26.0",
    "dayjs": "~1.11.6",
    "dropzone": "~6.0.0-beta.2",
    "js-pimcore-formbuilder": "~1.0.6",
    "pinia": "~2.0.23",
    "the-new-css-reset": "~1.7.3",
    "vue": "~3.2.41",
    "vue-i18n": "~9.2.2"
  },
  "devDependencies": {
    "@babel/core": "~7.19.6",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.19.4",
    "@babel/preset-typescript": "~7.18.6",
    "@faker-js/faker": "~7.6.0",
    "@storybook/addon-actions": "~6.5.16",
    "@storybook/addon-essentials": "~6.5.16",
    "@storybook/addon-interactions": "~6.5.16",
    "@storybook/addon-links": "~6.5.16",
    "@storybook/builder-vite": "~0.4.0",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "~6.5.16",
    "@types/jest": "~29.2.6",
    "@types/node": "~18.11.9",
    "@types/resize-observer-browser": "~0.1.6",
    "@typescript-eslint/eslint-plugin": "~5.51.0",
    "@typescript-eslint/parser": "~5.51.0",
    "@vitejs/plugin-vue": "~4.0.0",
    "@vue/eslint-config-typescript": "~11.0.2",
    "@vue/test-utils": "~2.2.1",
    "ajv": "~8.11.0",
    "autoprefixer": "~10.4.13",
    "babel-plugin-transform-require-context": "~0.1.1",
    "cssnano": "~5.1.14",
    "eslint": "~8.26.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-valantic": "~14.3.1",
    "eslint-import-resolver-webpack": "~0.13.2",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-storybook": "~0.6.10",
    "eslint-plugin-vue": "~9.7.0",
    "husky": "~8.0.1",
    "jsdom": "~21.1.0",
    "lint-staged": "~13.0.3",
    "mini-css-extract-plugin": "~2.6.1",
    "msw": "~0.47.4",
    "postcss": "~8.4.18",
    "postcss-html": "~1.5.0",
    "postcss-scss": "~4.0.5",
    "rollup-plugin-visualizer": "~5.9.0",
    "sass": "~1.55.0",
    "stylelint": "~14.14.0",
    "stylelint-config-valantic": "~7.1.4",
    "svg-sprite": "~2.0.1",
    "svgo": "~3.0.2",
    "typescript": "~4.9.4",
    "vite": "~4.0.4",
    "vite-plugin-image-optimizer": "~1.0.7",
    "vitest": "~0.28.4",
    "vue-router": "~4.1.6",
    "vue-tsc": "~1.0.24"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "not dead"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
