{
  "name": "vue-template",
  "version": "1.0.0-alpha.1",
  "description": "A customized Vue.js project template",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --env.production",
    "build:monitor": "npm run build -- --env.monitor",
    "build:profile": "rimraf dist && webpack --env.production --profile --json > stats/profile.json",
    "build:styleguide": "npm run build -- --env.styleguide && vue-styleguidist build",
    "eslint": "eslint --ext .js,.vue app test/unit/specs; exit 0;",
    "eslint:fix": "eslint --ext .js,.vue app test/unit/specs --fix; exit 0;",
    "stylelint": "stylelint '**/*.?(vue|scss)'; exit 0",
    "stylelint:fix": "echo \"Using stylelint --fix is not a good idea when using *.vue files...\"",
    "lint": "npm run eslint && npm run stylelint",
    "test:unit": "jest --config package.json; exit 0;",
    "test": "npm run lint && npm run test:unit",
    "dev": "webpack-dev-server",
    "dev:styleguide": "vue-styleguidist server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patric-eberle/vue-template.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "verst√§rker"
  ],
  "author": "Patric Eberle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patric-eberle/vue-template/issues"
  },
  "homepage": "https://github.com/patric-eberle/vue-template#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/$1"
    },
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "<rootDir>/tests/unit/setup"
    ],
    "mapCoverage": true,
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "collectCoverageFrom": [
      "app/components/**/*.{js,vue}",
      "app/plugins/**/*.{js,vue}",
      "!app/main.js",
      "!**/node_modules/**"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "vue": "^2.5.13",
    "vue-bem-cn": "^1.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.1.3",
    "jest-serializer-vue": "^0.3.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.7.0",
    "vue-style-loader": "^3.0.3",
    "vue-styleguidist": "^1.4.5",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-monitor": "^1.0.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
