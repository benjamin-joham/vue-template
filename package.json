  "description": "A customized Vue.js project template",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --env.production; exit 0;",
    "build:profile": "npm run clean && webpack --env.production --profile --json > webpack-stats.json; exit 0;",
    "eslint": "eslint --ext .js,.vue app test/unit/specs; exit 0;",
    "eslint:fix": "eslint --ext .js,.vue app test/unit/specs --fix; exit 0;",
