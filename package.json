{
  "name": "vue-template",
  "version": "4.0.0",
  "description": "A customized Vue.js project template",
  "scripts": {
    "clean:caches": "rm -r -f .eslintcache .stylelintcache node_modules/.cache",
    "dev": "webpack-dev-server --mode development",
    "dev:s": "npm run styleguide",
    "jest": "jest tests --config jest.config.js",
    "styleguide": "vue-styleguidist server",
    "test": "eslint --cache --ext .js,.vue src tests/unit/specs && stylelint --cache 'src/**/*.?(vue|scss)' && npm run jest; exit 0;",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "build:profile": "NODE_ENV=production webpack --mode production --profile",
    "build:styleguide": "webpack --styleguide-build && vue-styleguidist build",
    "build:watch": "NODE_ENV=production webpack --mode production --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run jest",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.{css,vue,scss}": [
      "stylelint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": "12.14",
    "npm": ">=6.13.1"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "axios": "~0.19.2",
    "core-js": "~3.6.4",
    "css-vars-ponyfill": "~2.2.1",
    "dayjs": "~1.8.23",
    "picturefill": "~3.0.3",
    "portal-vue": "~2.1.7",
    "swiper": "~5.3.6",
    "vue": "~2.6.11",
    "vue-i18n": "~8.15.5",
    "vue-js-modal": "~1.3.33",
    "vue-router": "~3.1.6",
    "vuex": "~3.1.3"
  },
  "devDependencies": {
    "@babel/core": "~7.10.5",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.10.4",
    "@vue/test-utils": "~1.0.0-beta.32",
    "autoprefixer": "~9.7.4",
    "axios-mock-adapter": "~1.17.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~10.1.0",
    "babel-jest": "~26.1.0",
    "babel-loader": "~8.1.0",
    "clean-webpack-plugin": "~3.0.0",
    "css-loader": "~3.4.2",
    "cssnano": "~4.1.10",
    "eslint": "~7.5.0",
    "eslint-config-airbnb-base": "~14.2.0",
    "eslint-config-valantic": "~8.0.0",
    "eslint-import-resolver-webpack": "~0.12.2",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-vue": "~6.2.2",
    "eslint-webpack-plugin": "~1.0.0",
    "faker": "~4.1.0",
    "file-loader": "~6.0.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~4.2.3",
    "image-webpack-loader": "~6.0.0",
    "jest": "~25.1.0",
    "jest-transform-stub": "~2.0.0",
    "launch-editor-middleware": "~2.2.1",
    "lint-staged": "~10.0.8",
    "mini-css-extract-plugin": "~0.9.0",
    "postcss-loader": "~3.0.0",
    "sass": "~1.26.3",
    "sass-loader": "~8.0.2",
    "sass-resources-loader": "~2.0.1",
    "stylelint": "~13.2.1",
    "stylelint-config-valantic": "~6.0.0",
    "stylelint-webpack-plugin": "~1.2.3",
    "stylus": "~0.54.7",
    "stylus-loader": "~3.0.2",
    "uglifyjs-webpack-plugin": "~2.2.0",
    "vue-jest": "~3.0.5",
    "vue-loader": "~15.9.0",
    "vue-markdown-loader": "~2.4.1",
    "vue-styleguidist": "~4.13.1",
    "webpack": "~4.42.0",
    "webpack-bundle-analyzer": "~3.6.1",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3",
    "webpack-manifest-plugin": "~2.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11"
  ]
}
