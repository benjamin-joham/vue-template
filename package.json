    "lint-staged": "~13.0.3",
    "mini-css-extract-plugin": "~2.6.1",
    "msw": "~0.47.4",
    "postcss": "~8.4.18",
    "postcss-html": "~1.5.0",
    "postcss-loader": "~7.0.1",
    "postcss-scss": "~4.0.5",
    "sass": "~1.55.0",
    "sass-loader": "~13.1.0",
    "stylelint": "~14.14.0",
    "stylelint-config-valantic": "~7.1.4",
    "stylelint-webpack-plugin": "~3.3.0",
    "svg-sprite": "~2.0.1",
    "svg4everybody": "~2.1.9",
    "terser-webpack-plugin": "~5.3.6",
    "ts-jest": "~29.0.3",
    "ts-loader": "~9.4.1",
    "typescript": "~4.8.4",
    "vite": "~4.0.4",
    "vue-loader": "~17.0.1",
    "vue-markdown-loader": "~2.5.0",
    "vue-router": "~4.1.6",
    "webpack": "~5.74.0",
    "webpack-assets-manifest": "~5.1.0",
    "webpack-bundle-analyzer": "~4.7.0",
    "webpack-cli": "~4.10.0",
    "webpack-dev-server": "~4.11.1",
    "webpack-env": "~0.8.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "not dead"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
